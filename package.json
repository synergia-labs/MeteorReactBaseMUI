{
	"name": "meteor-react-base-mui",
	"private": true,
	"scripts": {
		"start": "meteor run --settings settings.json",
		"visualize": "meteor --production --extra-packages bundle-visualizer",
		"lint": "npx eslint .",
		"lint:fix": "npx eslint . --fix",
		"newModule": "node ./.module/create.js",
		"prepare": "husky",
		"commit": "node -e \"const args = process.argv.slice(2).join(' '); require('child_process').execSync('git add ' + (args || '.') + ' && cz', {stdio: 'inherit'});\"",
		"commitlint": "commitlint --edit"
	},
